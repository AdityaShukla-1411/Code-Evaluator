{
  "version": 2,
  "name": "code-evaluator-unified",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@code-evaluator-api-url"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "@code-evaluator-api-url"
    }
  },
  "github": {
    "silent": true
  }
}
